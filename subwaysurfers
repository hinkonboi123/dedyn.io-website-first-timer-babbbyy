<script>
    document.addEventListener('DOMContentLoaded', function() {
        // Update favorites count
        const updateFavoritesCount = () => {
            const favorites = JSON.parse(localStorage.getItem('favorites') || '[]');
            const countElement = document.querySelector('.favorites-count');
            if (countElement && favorites.length > 0) {
                countElement.textContent = `(${favorites.length})`;
            } else if (countElement) {
                countElement.textContent = '';
            }
        };

        // Update recently played count
        const updateRecentlyPlayedCount = () => {
            const recentlyPlayed = JSON.parse(localStorage.getItem('recentlyPlayed') || '[]');
            const countElement = document.querySelector('.recently-played-count');
            if (countElement && recentlyPlayed.length > 0) {
                countElement.textContent = `(${recentlyPlayed.length})`;
            } else if (countElement) {
                countElement.textContent = '';
            }
        };

        // Initial updates
        updateFavoritesCount();
        updateRecentlyPlayedCount();

        // Listen for changes
        window.addEventListener('storage', function() {
            updateFavoritesCount();
            updateRecentlyPlayedCount();
        });
        
        window.addEventListener('favoritesUpdated', updateFavoritesCount);
        window.addEventListener('recentlyPlayedUpdated', updateRecentlyPlayedCount);
    });
</script>
