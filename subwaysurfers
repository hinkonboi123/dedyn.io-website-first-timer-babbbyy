<script>
window.addEventListener('load',function(){
    const s=document.createElement('script');
    s.async=true;
    s.src='https://www.googletagmanager.com/gtag/js?id=G-RJKB7W6H34';
    document.head.appendChild(s);
    
    s.onload=function(){
        window.dataLayer=window.dataLayer||[];
        function gtag(){dataLayer.push(arguments);}
        
        gtag('js',new Date());
        gtag('config','G-RJKB7W6H34',{
            page_title:'Subway Surfers - Play Free Online On Tops.Games',
            page_location:'https://tops.games/game/subway-surfers',
            send_page_view:true,
            anonymize_ip:true
        });
        
        const data={
            page_type:'game',
            site_section:'game'
        };
        
                data.game_title='Subway Surfers';
        data.game_category='Adventure';
        data.game_views=1829;
                
        gtag('event','page_view',data);
        
        let start=Date.now();
        window.addEventListener('beforeunload',function(){
            const time=Math.round((Date.now()-start)/1000);
            if(time>5){
                gtag('event','user_engagement',{
                    engagement_time_msec:time*1000,
                    page_type:'game'
                });
            }
        });
    };
});
</script>

