<script>document.addEventListener("DOMContentLoaded",(()=>{const e=document.getElementById("sidebar"),t=document.getElementById("sidebar-toggle"),s=document.querySelector(".page-container");let i=0,n=0,a=0,o=!1,c=0,d=0;const r=()=>window.innerWidth<=768,l=()=>{s.classList.remove("sidebar-hidden"),s.classList.add("sidebar-open"),e.style.transform="",e.style.transition=""},h=()=>{s.classList.remove("sidebar-open"),s.classList.add("sidebar-hidden"),e.style.transform="",e.style.transition=""},u=t=>{if(!r())return;const n=t.touches[0];i=n.clientX,a=n.clientY,touchStartTime=Date.now();const l=i<=20&&s.classList.contains("sidebar-hidden"),h=e.getBoundingClientRect(),u=i>=h.left&&i<=h.right&&a>=h.top&&a<=h.bottom&&s.classList.contains("sidebar-open");(l||u)&&(o=!0,c=l?-e.offsetWidth:0,d=c,e.style.transition="none",l&&s.classList.add("edge-swipe-active"))},m=t=>{if(!o||!r())return;const n=t.touches[0],l=n.clientX-i,h=n.clientY-a;if(Math.abs(h)>Math.abs(l)&&Math.abs(h)>10)return void L();Math.abs(l)>10&&t.preventDefault();let u=c+l;u=(s.classList.contains("sidebar-hidden"),Math.max(-e.offsetWidth,Math.min(0,u))),d=u,e.style.transform=`translateX(${u}px)`;const m=Math.abs(u)/e.offsetWidth;b(1-m)},v=t=>{if(!o||!r())return;const s=Date.now()-touchStartTime,a=t.changedTouches[0];n=a.clientX;const c=n-i,u=Math.abs(c)/s>.3?c>0:d>-e.offsetWidth/2;e.style.transition="transform 0.3s ease-out",u?l():h(),setTimeout((()=>{e.style.transform="",e.style.transition=""}),300),L()},L=()=>{o=!1,s.classList.remove("edge-swipe-active"),b(s.classList.contains("sidebar-open")?1:0)},b=e=>{const t=document.querySelector(".sidebar-overlay");t&&(t.style.opacity=e)};if(e&&t&&s){t.addEventListener("click",(e=>{e.stopPropagation(),t.classList.add("touched"),setTimeout((()=>t.classList.remove("touched")),200),r()?s.classList.contains("sidebar-hidden")?l():h():s.classList.toggle("sidebar-hidden")})),e.addEventListener("mouseenter",(()=>{r()||s.classList.contains("sidebar-hidden")||s.classList.add("sidebar-o