tEnhancedGame(),gameStarted=!0,gameLoaded=!0,s&&(s.innerHTML="<span>⏸️</span> Pause",s.onclick=pauseGame),"undefined"!=typeof gtag&&gtag("event","game_start",{game_name:gameData.title,game_category:gameData.category,game_type:gameData.type||"unknown"})}),800)):(requestAnimationFrame((()=>{e&&e.classList.add("loading-game"),t&&(t.style.display="none"),n&&(n.style.display="none"),o&&(o.style.display="block"),s&&(s.innerHTML="<span>⏸️</span> Playing",s.classList.add("active"))})),setTimeout((()=>{requestAnimationFrame((()=>{a.src=a.dataset.src,a.style.display="block",o&&(o.style.display="none")})),gameStarted=!0,gameLoaded=!0,"undefined"!=typeof gtag&&gtag("event","game_start",{game_name:gameData.title,game_category:gameData.category})}),500))}function pauseGame(){const e=document.getElementById("playBtn");e&&(e.innerHTML="<span>▶️</span> Play",e.onclick=resumeGame);const t=document.getElementById("gameFrame");if(t&&t.contentWindow)try{t.contentWindow.postMessage({action:"pause"},"*")}catch(e){}}function resumeGame(){const e=document.getElementById("playBtn");e&&(e.innerHTML="<span>⏸️</span> Pause",e.onclick=pauseGame);const t=document.getElementById("gameFrame");if(t&&t.contentWindow)try{t.contentWindow.postMessage({action:"resume"},"*")}catch(e){}}function loadGame(){startGame()}function toggleFullscreen(){const e=document.getElementById("gameWrapper"),t=document.getElementById("fullscreenBtn");isFullscreen?(document.exitFullscreen?document.exitFullscreen():document.webkitExitFullscreen?document.webkitExitFullscreen():document.msExitFullscreen&&document.msExitFullscreen(),t&&(t.innerHTML="<span>⛶</span> Fullscreen"),isFullscreen=!1):(e.requestFullscreen?e.requestFullscreen().catch((e=>{})):e.webkitRequestFullscreen?e.webkitRequestFullscreen():e.msRequestFullscreen&&e.msRequestFullscreen(),t&&(t.innerHTML="<span>⛶</span> Exit Fullscreen"),isFullscreen=!0)}function reloadGame(){if(gameStarted){const e=document.getElementById("gameFrame"),t=window.gameLoaderConfig;t&&t.url?(e.src="",setTimeout((()=>{e.src=t.url}),100)):e.src=e.src}else startGame()}function toggleSound(){const e=document.getElementById("soundBtn");if(isMuted=!isMuted,isMuted?e&&(e.innerHTML="<span>🔇</span> Muted",e.classList.add("active")):e&&(e.innerHTML="<span>🔊</span> Sound",e.classList.remove("active")),gameStarted){const e=document.getElementById("gameFrame");if(e&&e.contentWindow)try{e.contentWindow.postMessage({action:"toggleSound",muted:isMuted},"*")}catch(e){}}}function reportIssue(){window.location.href=`/report?game=${gameData.slug}`}function showQuickReportModal(){const e=document.createElement("div");e.className="quick-report-modal",e.innerHTML=`\n    <div class="quick-report-content">\n      <button class="quick-report-close" onclick="closeQuickReport()">✕</button>\n      <h3>🚨 Quick Report</h3>\n      <p>What's wrong with "${gameData.title}"?</p>\n      <div class="quick-report-options">\n        <button onclick="quickReport('broken')" class="quick-report-btn">\n          🔧 Game Not Working\n        </button>\n        <button onclick="quickReport('inappropriate')" class="quick-report-btn">\n          ⚠️ Inappropriate Content\n        </button>\n        <button onclick="quickReport('spam')" class="quick-report-btn">\n          🚫 Spam/Low Quality\n        </button>\n        <button onclick="openFullReport()" class="quick-report-btn secondary">\n          📝 Detailed Report\n        </button>\n      </div>\n    </div>\n  `,document.body.appendChild(e),e.classList.add("active")}function quickReport(e){fetch("/api/report.php",{method:"POST",headers:{"Content-Type":"application/json"},body:JSON.stringify({reportable_type:"game",reportable_id:gameData.id,reason:e,description:{broken:"Game is not working properly",inappropriate:"Game contains inappropriate content",spam:"Game appears to be spam or low quality"}[e]+" (Quick report)",reporter_email:""})}).then((e=>e.json())).then((e=>{closeQuickReport(),e.success?alert("✅ "+e.message):alert("❌ "+e.message)})).catch((e=>{closeQuickReport(),alert("❌ Failed to submit report. Please try the detailed report page.")}))}function openFullReport(){closeQuickReport(),window.location.href=`/report?game=${gameData.slug}`}function closeQuickReport(){const e=document.querySelector(".quick-report-modal");e&&e.remove()}function switchTab(e){document.querySelectorAll(".tab-content").forEach((e=>{e.classList.remove("active")})),document.querySelectorAll(".tab").forEach((e=>{e.classList.remove("active")}));const t=document.getElementById(e+"-tab");t&&t.classList.add("active"),event&&event.target&&event.target.classList.add("active")}function toggleFavorite(){const e=document.getElementById("favIcon"),t=document.getElementById("favText");if(!e||!t)return;const n="❤️"===e.textContent;n?(e.textContent="🤍",t.textContent="Favorite"):(e.textContent="❤️",t.textContent="Favorited");const a=JSON.parse(localStorage.getItem("favorites")||"[]");if(n){const e=a.indexOf(gameData.slug);e>-1&&a.splice(e,1)}else a.push(gameData.slug);localStorage.setItem("favorites",JSON.stringify(a)),window.dispatchEvent(new Event("favoritesUpdated"))}function openShareModal(){const e=document.getElementById("shareModal");e&&e.classList.add("active")}function closeShareModal(){const e=document.getElementById("shareModal");e&&e.classList.remove("active")}function shareOn(e){const t=encodeURIComponent(window.location.href),n=encodeURIComponent(`${gameData.title} - Play Free Online`);let a="";switch(e){case"facebook":a=`https://www.facebook.com/sharer/sharer.php?u=${t}`;break;case"twitter":a=`https://twitter.com/intent/tweet?text=${n}&url=${t}`;break;case"whatsapp":a=`https://wa.me/?text=${n}%20${t}`;break;case"reddit":a=`https://reddit.com/submit?url=${t}&title=${n}`;break;case"telegram":a=`https://t.me/share/url?url=${t}&text=${n}`;break;case"copy":return navigator.clipboard.writeText(window.location.href).then((function(){alert("Link copied to clipboard!"),closeShareModal()})),!1}return a&&window.open(a,"_blank","width=600,height=400"),closeShareModal(),!1}function submitComment(e){e.preventDefault();const t=new FormData(e.target);fetch("/api/interactions_add.php",{method:"POST",body:t}).then((e=>e.json())).then((t=>{t.success?(alert(t.message||"Comment submitted successfully!"),e.target.reset(),selectedRating=0,document.querySelectorAll(".rating-input .star").forEach((e=>{e.classList.remove("selected"),e.style.color=""})),location.reload()):alert(t.message||"Failed to submit comment. Please try again.")})).catch((e=>{alert("An error occurred. Please try again.")}))}function initRatingSystem(){document.querySelectorAll(".rating-input .star").forEach((e=>{e.addEventListener("click",(function(){selectedRating=parseInt(this.dataset.rating);const e=document.getElementById("ratingValue");e&&(e.value=selectedRating),document.querySelectorAll(".rating-input .star").forEach(((e,t)=>{t<selectedRating?e.classList.add("selected"):e.classList.remove("selected")}))})),e.addEventListener("mouseenter",(function(){const e=parseInt(this.dataset.rating);document.querySelectorAll(".rating-input .star").forEach(((t,n)=>{t.style.color=n<e?"#fbbf24":""}))}))}));const e=document.querySelector(".rating-input");e&&e.addEventListener("mouseleave",(function(){document.querySelectorAll(".rating-input .star").forEach(((e,t)=>{e.style.color=t<selectedRating?"#fbbf24":""}))}))}function initModalEvents(){const e=document.getElementById("shareModal");e&&e.addEventListener("click",(function(e){e.target===this&&closeShareModal()})),document.addEventListener("keydown",(function(e){if("Escape"===e.key){const e=document.getElementById("shareModal");e&&e.classList.contains("active")&&closeShareModal()}}))}function initFavorites(){if(JSON.parse(localStorage.getItem("favorites")||"[]").includes(gameData.slug)){const e=document.getElementById("favIcon"),t=document.getElementById("favText");e&&(e.textContent="❤️"),t&&(t.textContent="Favorited")}}function initSessionTracking(){window.addEventListener("beforeunload",(function(){const e=Math.round((Date.now()-startTime)/1e3);"undefined"!=typeof gtag&&e>10&&gtag("event","game_session",{game_name:gameData.title,session_duration:e,game_type:gameData.type||"unknown"})}))}function debugRecentlyPlayed(){return JSON.parse(localStorage.getItem("recentlyPlayed")||"[]")}window.gameLoaderConfig=window.gameLoaderConfig||{},window.debugRecentlyPlayed=debugRecentlyPlayed,document.addEventListener("DOMContentLoaded",(function(){initRatingSystem(),initModalEvents()})),window.addEventListener("load",(function(){initFavorites(),initSessionTracking(),window.gameLoaderConfig}));
